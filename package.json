{
  "name": "leaflet-terminator",
  "version": "0.2.0",
  "description": "show the solar terminator on a leaflet map",
  "main": "index.js",
  "scripts": {
    "start": "beefy demo.js",
    "make": "browserify -s terminator index.js | uglifyjs -c -m > leaflet-terminator.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapbox/leaflet-terminator.git"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "keywords": [
    "leaflet",
    "solar",
    "terminator"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/leaflet-terminator/issues"
  },
  "homepage": "https://github.com/mapbox/leaflet-terminator",
  "devDependencies": {
    "beefy": "~2.1.0",
    "leaflet": "~0.7.3",
    "brfs": "1.1.0",
    "watchify": "~1.0.0",
    "insert-css": "~0.2.0",
    "uglifyjs": "~2.4.10"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "leaflet-geodesy": "0.2.1",
    "spherical": "0.0.1"
  }
}
